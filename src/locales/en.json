{
	"greeting": "Ultimate Media Manager\n<a href=\"https://github.com/ExposedCat/ultimate-media-manager\">💻 Source code</a>\n⚙️ Chat /settings\n\nPowered by @ExposedCatDev",
	"viewOn": {
		"facebook": "<a href=\"tg://user?id=${userId}\">${userName}</a> sent <a href=\"${postUrl}\">this Facebook ${kind}</a>",
		"instagram": "<a href=\"tg://user?id=${userId}\">${userName}</a> sent <a href=\"${postUrl}\">this Instagram ${kind}</a>",
		"tiktok": "<a href=\"tg://user?id=${userId}\">${userName}</a> sent <a href=\"${postUrl}\">this TikTok ${kind}</a>",
		"youtube": "<a href=\"tg://user?id=${userId}\">${userName}</a> sent <a href=\"${postUrl}\">this YouTube Short ${kind}</a>",
		"twitter": "<a href=\"tg://user?id=${userId}\">${userName}</a> sent <a href=\"${postUrl}\">this Twitter post</a>",
		"pinterest": "<a href=\"tg://user?id=${userId}\">${userName}</a> sent <a href=\"${postUrl}\">this Pinterest pin</a>",
		"soundcloud": "<a href=\"tg://user?id=${userId}\">${userName}</a> sent <a href=\"${postUrl}\">this SoundCloud song</a>",
		"reddit": "<a href=\"tg://user?id=${userId}\">${userName}</a> sent <a href=\"${postUrl}\">this Reddit post</a>",
		"null": "<a href=\"tg://user?id=${userId}\">${userName}</a> sent <a href=\"${postUrl}\">this video</a>"
	},
	"rawCaption": "${kind} ${type}",
	"rawErrorCaption": "${kind} ${type}\n\n<i>${error}</i>",
	"status": {
		"downloading": {
			"audio": "🔉 Downloading audio...",
			"video": "🎥 Downloading video..."
		}
	},
	"downloaded": {
		"audio": "🔉 <a href=\"${url}\">${title}</a>",
		"video": "🎥 <a href=\"${url}\">${title}</a>"
	},
	"error": {
		"viewOn": "${viewOn}\n\n<i>${error}</i>",
		"audio": "😢 Failed to download audio",
		"video": "😢 Failed to download video",
		"videoSize": "🫨 Video is too big to download: ${size} Mb out of ${limit} Mb limit"
	},
	"promoCaption": "${viewUrl}",
	"settings": "Chat settings:\n\n${cleanup} ${cleanupOption} (<code>cleanup</code>)\n\n/settings OPTION yes|no",
	"unknownOption": "Unknown option \"${option}\"\nSee /settings",
	"cleanupOption": "Delete links",
	"settingsUpdated": "\"${option}\" was set to \"${value}\"",
	"enabled": "🟢",
	"disabled": "🔴"
}
